<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.msp.sample" >
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name="com.manusunny.pinlock.sample.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.manusunny.pinlock.sample.SetPinActivitySample" />
        <activity android:name="com.manusunny.pinlock.sample.ConfirmPinActivitySample" />

    </application>

</manifest>
